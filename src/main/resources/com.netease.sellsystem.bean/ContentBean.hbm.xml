<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.netease.sellsystem.bean.ContentBean" table="content" schema="sellsystem">
        <id name="contentId" column="content_id"/>
        <property name="title" column="title"/>
        <property name="contentPic" column="content_pic"/>
        <property name="summary" column="summary"/>
        <property name="text" column="text"/>
        <property name="userId" column="user_id"/>
        <property name="price" column="price"/>
        <property name="saleQuantity" column="sale_quantity"/>
        <property name="key1" column="key1"/>
        <property name="key2" column="key2"/>
        <property name="key3" column="key3"/>
    </class>
</hibernate-mapping>