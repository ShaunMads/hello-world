<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.netease.sellsystem.bean.OrderDetailsBean" table="order_details" schema="sellsystem">
        <id name="odetailsId" column="odetails_id"/>
        <property name="contentId" column="content_id"/>
        <property name="quantity" column="quantity"/>
        <property name="orderId" column="order_id"/>
        <property name="title" column="title"/>
        <property name="price" column="price"/>
        <property name="contentPic" column="content_pic"/>
        <property name="key1" column="key1"/>
        <property name="key2" column="key2"/>
        <property name="key3" column="key3"/>
    </class>
</hibernate-mapping>